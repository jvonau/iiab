[Unit]
Description=MySql config service
After=mariadb.service
Requires=mariadb.service

[Service]
Type=oneshot
WorkingDirectory=/opt/iiab/iiab
ExecStart=/opt/iiab/iiab/runrole mysql-setup /opt/iiab/iiab/mysql.log
ExecStartPost=/bin/systemctl disable iiab-setup-db.service

[Install]
WantedBy=multi-user.target
